<div id="wiki-wrapper" class="results">
<div id="head" style="border:none;">
  <h1>{{title}}</h1>
</div>
<div id="wiki-content">
{{#has_results}}
  <div id="results">
  {{{results}}}
  </div>
{{/has_results}}

{{#no_results}}
  <p id="no-results">
    There are no pages in <strong>{{ref}}</strong>.
  </p>
{{/no_results}}
</div>
</div>
